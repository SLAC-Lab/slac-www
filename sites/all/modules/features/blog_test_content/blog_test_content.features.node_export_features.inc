      \'title\' => \'Writing for the Web #1: Calls to Action\',
      \'log\' => \'\',
      \'status\' => \'1\',
      \'comment\' => \'2\',
      \'promote\' => \'1\',
      \'sticky\' => \'0\',
      \'vuuid\' => \'aef4e039-6a42-46b9-b42f-9c170b47efad\',
      \'nid\' => \'7414\',
      \'type\' => \'blog\',
