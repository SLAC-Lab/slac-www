<?php

/**
 * Implementation of hook_install.
 */
function slac_site_variables_install() {
  variable_set('employee_portal_url', '');
  variable_set('research_resources_url', '');
}

/**
 * Implementation of hook_uninstall.
 */
function slac_site_variables_uninstall() {
  variable_del('employee_portal_url');
  variable_del('research_resources_url');
}
