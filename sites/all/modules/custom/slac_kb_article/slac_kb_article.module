include_once 'slac_kb_article.features.inc';

/**
 * Implements hook_ctools_plugin_directory().
 */
function slac_kb_article_ctools_plugin_directory($module, $plugin) {
  if ($module === 'ctools') {
    return 'plugins/' . $plugin;
  }
}
